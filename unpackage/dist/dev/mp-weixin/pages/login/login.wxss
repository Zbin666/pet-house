
.login-page.data-v-e4e4508d {
		height: 100vh;
		padding: 96rpx 48rpx 48rpx;
		box-sizing: border-box;
        background: url('data:image/png;base64,') center/cover no-repeat fixed,
                    linear-gradient(180deg, #ffe082 0%, #fff6cc 28%, #fff9e6 60%, #fffcee 100%);
		position: relative;
}
.logo.data-v-e4e4508d {
        width: 308rpx;
        display: block;
        margin: 200rpx auto 24rpx; /* 往下移动 */
}
.title.data-v-e4e4508d {
        text-align: center;
        color: #1a1a1a; /* 调深标题颜色 */
        font-size: 40rpx;
		font-weight: 550;
        margin-bottom: 80rpx;
}
.btn-group.data-v-e4e4508d {
        display: flex;
        flex-direction: column;
        gap: 28rpx;
        align-items: center;
        margin: 0 16rpx;
}
.btn.data-v-e4e4508d {
		border-radius: 999rpx;
		line-height: 96rpx;
		height: 96rpx;
        font-size: 34rpx;
		text-align: center;
        border: 4rpx solid #2c2c2c; /* 深色描边 */
		background-color: #ffd54f;
        color: #0e0e0e; /* 进一步加深按钮文字 */
        width: 620rpx; /* 增加并统一按钮长度 */
        box-shadow: 0 2rpx 0 rgba(0,0,0,0.06);
}

    /* 确保小程序端文字颜色不被默认样式覆盖 */
.btn text.data-v-e4e4508d { color: #0e0e0e;
}
.btn.wx.data-v-e4e4508d {
		background-color: #ffd54f;
}
.btn.phone.data-v-e4e4508d {
		background-color: #ffe082;
}
.btn.data-v-e4e4508d:disabled {
		opacity: 0.6;
}
.icon.data-v-e4e4508d {
		margin-right: 8rpx;
}
.icon-img.data-v-e4e4508d {
        width: 40rpx;
        height: 40rpx;
        margin-right: 12rpx;
        vertical-align: middle;
}
.agreement.data-v-e4e4508d {
		margin-top: 36rpx;
		font-size: 22rpx;
		color: #888;
		display: flex;
		align-items: center;
		justify-content: center;
		flex-wrap: wrap;
		gap: 4rpx;
}
.checkbox.data-v-e4e4508d {
		width: 32rpx;
		height: 32rpx;
		border-radius: 50%;
		border: 2rpx solid #bbb;
		margin-right: 12rpx;
		position: relative;
}
.checkbox.checked.data-v-e4e4508d {
		background-color: #ffd54f;
		border-color: #f1c40f;
}
.link.data-v-e4e4508d {
		color: #f1a400;
}

